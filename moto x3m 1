<script>
  var IOS_DEVICE = false;
  var game_embed_code = `<object id="game_frame" scrolling="no" height="100%" width="100%" frameborder="0" seamless="true" webkitallowfullscreen="true" mozallowfullscreen="true" allowfullscreen="true" webkit-playsinline="true" data="https://html5.gamedistribution.com/5b0abd4c0faa4f5eb190a9a16d5a1b4c/"></object>
  `;
  const gameContainer = document.getElementById('game-container');
  game_embed_code = helpers.add_referrer_to_embedded_url(game_embed_code);
  gameContainer.insertAdjacentHTML("beforeend", game_embed_code);

  let hideBtn = document.querySelector("#game-container #hide-game-btn");
  hideBtn.onclick = exitFullscreen;

  document.addEventListener('touchmove', function handleTouchEvent(e) {
    if(!hideBtn || e.target !== hideBtn || e.touches.length === 0) return;
    e.stopPropagation();
    let BTN_HEIGHT = 50,
      newY = e.touches[0].clientY - BTN_HEIGHT / 2,
      MIN_TOP = 0,
      MAX_TOP = window.innerHeight - BTN_HEIGHT,
      btnTop = Math.min(Math.max(newY, MIN_TOP), MAX_TOP);
    hideBtn.style.top = btnTop + 'px';
  }, true);

  const ply_btn = document.getElementById('play_now');
  const play = () => {
    const game_embed = document.getElementById('game_frame');
    const child_window = game_embed && game_embed.contentWindow;

    requestFullscreen();
    ply_btn.innerText = 'CONTINUE PLAYING';
    ply_btn.onclick = requestFullscreen;

    child_window && window.addEventListener('message', message => {
      if (message.source !== child_window) return;
      if (message.data === 'scrollToWalkthrough') {
        exitFullscreen();
        document.getElementById('VideoEmbed').scrollIntoView(true);
        gtag_send_event('walkthrough', {
          'event_category': 'Go to Walkthrough Video'
        });
      }
    });
  };

  function requestFullscreen(){
    if(IOS_DEVICE){
      gameContainer.style.height = window.innerHeight + 'px';
      document.body.classList.add('ios-fullscreen');
      window.onresize = () => {
        gameContainer.style.height = window.innerHeight + 'px';
        hideBtn.style.top = '20px';
      };
    }else{
      gameContainer.requestFullscreen();
      window.onresize = () => hideBtn.style.top = '20px';
    }
  }

  function exitFullscreen(){
    if(IOS_DEVICE){
      gameContainer.style.height = '';
      document.body.classList.remove('ios-fullscreen');
    }else{
      document.exitFullscreen();
    }
    window.onresize = null;
  }

  ply_btn.onclick = play;

  this.shareopen = document.querySelectorAll('.at-share-btn');
  this.shareopenItems = [].slice.call(this.shareopen);
  this.shareopenItems.forEach(function (item, idx) {

    item.addEventListener('click', function (event) {
      event.preventDefault();
      console.log("item clicked")
      window.open(this.getAttribute("href"), 'ShareWindow', 'height=450, width=550, top=' + (window.outerHeight / 2 - 275) + ', left=' + (window.outerWidth / 2 - 225));
      //return false;
    });
  });
</script>





